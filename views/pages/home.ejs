<%- include('../includes/head.ejs') -%>

<body>
    <div class="container">
        <h2>Band List</h2>
        <div>
            <a href="/create/band">Create</a>
        </div>
        <div class="row">
            <div class="col-md-8">
                <h3>Band Name</h3>
            </div>
            <div class="col-md-2">
                <h3>Type</h3>

            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                <% for(var band of bands) { %>
                    <li> <%= band.name %> </li>
                    <% } %>
            </div>
            <div class="col-md-2">
                <% for(var band of bands) { %>
                    <li> <%= band.type %> </li>
                    <% } %>
            </div>
            <div class="col-md-2">
                <i class="fa fa-pencil" aria-hidden="true"></i>
                <i class="fa fa-trash" aria-hidden="true"></i>
            </div>

        </div>

    </div>

<%- include('../includes/foot.ejs') -%>